{
   "name": "hljs-enhance",
   "version": "1.0.5",
   "description": "Adds a little extra stuff to highlight.js",
   "license": "MIT",
   "type": "module",
   "main": "hljs-enhance.js",
   "repository": "github:center-key/hljs-enhance",
   "homepage": "https://github.com/center-key/hljs-enhance",
   "bugs": "https://github.com/center-key/hljs-enhance/issues",
   "docs": "https://github.com/center-key/hljs-enhance#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "highlightjs",
      "javascript",
      "syntax-highlighting"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "globals": {
         "globalThis": true
      }
   },
   "scripts": {
      "pretest": "html-validator",
      "test": "jshint . --exclude-path .gitignore"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "~6.2",
      "dna.js": "~2.0",
      "highlight.js": "~11.6",
      "jquery": "~3.6",
      "jshint": "~2.13",
      "w3c-html-validator": "~1.1",
      "web-ignition": "~1.5"
   }
}
